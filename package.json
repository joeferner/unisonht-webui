{
  "name": "@unisonht/webui",
  "version": "0.0.2",
  "description": "UnisonHT Web UI Plugin",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "prepare": "rm -rf dist && npm run build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix --ext .ts .",
    "lint-staged": "lint-staged",
    "pre-commit": "npm run format && npm run lint-staged && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeferner/unisonht-webui.git"
  },
  "keywords": [
    "unisonht",
    "webui"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeferner/unisonht-webui/issues"
  },
  "homepage": "https://github.com/joeferner/unisonht-webui#readme",
  "dependencies": {
    "@babel/standalone": "7.11.x",
    "@fortawesome/fontawesome-free": "5.14.x",
    "@material-ui/core": "4.11.x",
    "@unisonht/unisonht": "0.0.x",
    "axios": "0.20.x",
    "bootstrap": "4.5.x",
    "debug": "4.2.x",
    "ejs": "3.1.x",
    "find-up": "5.0.x",
    "jquery": "3.5.x",
    "less": "3.12.x",
    "react": "16.13.x",
    "react-dom": "16.13.x",
    "react-router-dom": "5.2.x"
  },
  "devDependencies": {
    "@types/debug": "4.x",
    "@types/ejs": "3.x",
    "@types/node": "14.x",
    "@types/yargs": "15.x",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "eslint": "7.x",
    "husky": "4.x",
    "lint-staged": "10.x",
    "prettier": "2.x",
    "ts-node": "9.x",
    "typescript": "4.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
